FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet//inlet
    {
		type		            fixedValue;
		value		            uniform 38.4;//100kPa;
    }
    outlet//outlet
    {
        type		            fixedValue;
        value		            uniform 0;
    }
    lowerNozzleWall
    {
        type		            zeroGradient;
    }
    middleNozzleWall
    {
        type		            zeroGradient;
    }
    upperNozzleWall
    {
        type		            zeroGradient;
    }
    back//wedgeFront
    {
        type                    wedge;//wedgeFront
    }
    front//wedgeBack
    {
        type                    wedge;//wedgeBack
    }
}

